  "bugs": {
    "url": "https://github.com/modulesio/webaverse-server/issues"
  },
  "homepage": "https://github.com/modulesio/webaverse-server#readme",
  "dependencies": {
    "aws-sdk": "^2.750.0",
    "bip39": "^3.0.2",
    "dotenv": "^8.2.0",
    "ethereumjs-wallet": "^1.0.1",
    "express": "^4.17.1",
    "express-jsdoc-swagger": "^1.5.0",
    "form-urlencoded": "^4.2.1",
    "http-proxy": "^1.18.1",
    "jsonwebtoken": "^8.5.1",
    "mime": "^2.4.6",
    "mkdirp": "^1.0.4",
    "node-fetch": "^2.6.0",
    "ps-node": "^0.1.6",
    "puppeteer": "^5.3.1",
    "redis": "^3.1.1",
    "redis-redisearch": "^1.0.1",
    "sha3": "^2.1.3",
    "uuid": "^8.3.2",
    "web3": "^1.3.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "eslint": "^7.24.0",
    "forever": "^3.0.0",
    "nodemon": "^2.0.7"
  }
}
