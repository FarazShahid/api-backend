{
  "name": "api-backend",
  "version": "0.0.1",
  "description": "Webaverse API server",
  "main": "src/index.js",
  "scripts": {
    "start": "sudo $(which node) $(which forever) start -a -l forever.log -o stdout.log -e stderr.log src/legacy/index.js",
    "stop": "sudo $(which node) $(which forever) stop src/legacy/index.js",
    "dev": "sudo $(which node) src/legacy/index.js",
    "dev-api": "nodemon src/api/index.js",
    "api": "cross-env PRODUCTION=true node src/api/index.js",
    "cache-server": "cross-env PRODUCTION=true node src/cache/index.js",
    "ipfs-server": "cross-env PRODUCTION=true node src/ipfs/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webaverse/api-backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/webaverse/api-backend"
  },
  "homepage": "https://github.com/webaverse/api-backend#readme",
  "dependencies": {
    "@pinata/sdk": "^1.1.13",
    "aws-sdk": "^2.750.0",
    "bip39": "^3.0.2",
    "cross-env": "^7.0.3",
    "dotenv-flow": "^3.2.0",
    "ethereumjs-wallet": "^1.0.1",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-jsdoc-swagger": "^1.5.0",
    "form-urlencoded": "^4.2.1",
    "http-proxy": "^1.18.1",
    "jsonwebtoken": "^8.5.1",
    "mime": "^2.4.6",
    "mkdirp": "^1.0.4",
    "node-fetch": "^2.6.0",
    "ps-node": "^0.1.6",
    "redis": "^3.1.1",
    "redis-redisearch": "^1.0.1",
    "sha3": "^2.1.3",
    "uuid": "^8.3.2",
    "web3": "^1.3.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "eslint": "^7.24.0",
    "forever": "^0.10.10",
    "nodemon": "^2.0.7"
  }
}
