{
  "name": "api-backend",
  "version": "0.0.1",
  "description": "Webaverse API server",
  "main": "src/index.js",
  "scripts": {
    "start": "sudo $(which node) $(which forever) start -a -l forever.log -o stdout.log -e stderr.log src/index.js",
    "stop": "sudo $(which node) $(which forever) stop src/index.js",
    "dev": "sudo $(which node) src/index.js",
    "dev-api-server": "nodemon src/api/v1.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/modulesio/webaverse-server.git"
  },
  "author": "",
